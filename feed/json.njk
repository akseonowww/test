---
# Metadata comes from _data/metadata.json
permalink: "{{ metadata.jsonfeed.path }}"
eleventyExcludeFromCollections: true
---

{ "version": "https://jsonfeed.org/version/1.1", "title": "{{ metadata.title
}}", "language": "{{ metadata.language }}", "home_page_url": "{{ metadata.url
}}", "feed_url": "{{ metadata.jsonfeed.url }}", "description": "{{
metadata.description }}", "author": { "name": "{{ metadata.author.name }}",
"url": "{{ metadata.author.url }}" }, "items": [ {%- for app in collections.apps
| reverse %} {%- set absoluteappUrl %}{{ app.url | url |
absoluteUrl(metadata.url) }}{% endset -%} { "id": "{{ absoluteappUrl }}", "url":
"{{ absoluteappUrl }}", "title": "{{ app.data.title }}", "content_html": {% if
app.templateContent %}{{ app.templateContent | dump | safe }}{% else %}""{%
endif %}, "date_published": "{{ app.date | rssDate }}" } {%- if not loop.last
-%} , {%- endif -%} {%- endfor %} ] }
